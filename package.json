  "description": "An ES6 Iterable utility library with TypeScript support",
  "main": "dist/lib/index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "babel-runtime": "^5.8.20"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "chai": "^3.0.0",
